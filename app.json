{
  "expo": {
    "name": "KATIG",
    "slug": "katig-mobile-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "scheme": "katig-app",
    "plugins": ["expo-router"],
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      },
      "preview2": {
        "android": {
          "gradleCommand": ":app:assembleRelease"
        }
      },
      "preview3": {
        "developmentClient": true
      },
      "preview4": {
        "distribution": "internal"
      },
      "production": {}
    },
    "extra": {
      "eas": {
        "projectId": "83ce1470-32d0-46cf-a40f-8c190e1f9c99"
      }
    },
    "android": {
      "package": "com.itsmenoahpoli.katigmobileapp"
    }
  }
}
